<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h1>form (input)</h1>
	<form>
		<!-- 텍스트 필드 -->
		<input /> <br /> <input type="text" />
		<!-- 위와 같은 역할을 하는 코드 -->

		<!-- 수 입력 필드 -->
		<input type="number" /> <br />

		<!-- 이메일 형식만 입력 -->
		<input type="email" /> <br />

		<!-- 날짜 형식 -->
		<input type="date" /> <br />

		<!-- 날짜와 시간 -->
		<input type="datetime-local" /> <br />

		<!-- 전송 버튼 -->
		<input type="submit" />

		<!-- 그냥 버튼 -->
		<input type="button" value="그냥 버튼" /> <br />

		<!-- 전송 버튼 -->
		<button>전송버튼</button>

		<!-- 그냥 버튼 -->
		<button type="button" >그냥 버튼 </button>



	</form>
</body>
</html>